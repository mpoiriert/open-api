{
    "name": "draw/open-api",
    "type": "library",
    "description": "Open Api PHP manipulation library. Only support v 2.0",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Draw\\Component\\OpenApi\\": ""
        }
    },
    "require": {
        "php": "^7.2",
        "ext-json": "*",
        "symfony/validator": "^3.4|^4.3|^5.0",
        "jms/serializer": "^3.0",
        "phpdocumentor/reflection-docblock": "^4.0|^5.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0",
        "doctrine/orm": "^2.7",
        "doctrine/common": "^2.12"
    },
    "conflict": {
        "draw/swagger": "*"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.8-dev"
        }
    }
}
